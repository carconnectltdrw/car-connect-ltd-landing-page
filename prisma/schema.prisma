// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Admin {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model App {
  id          Int    @id @default(autoincrement())
  name        String?
  description String?
}